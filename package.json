{
  "name": "homegames",
  "version": "1.0.0",
  "description": "Play games at home",
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "targets": [
      "node18-win-arm64",
      "node18-win-x64",
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-linux-arm64",
      "node18-linux-x64"
    ],
    "assets": []
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "homegames-common": "1.0.7",
    "homegames-core": "1.0.25",
    "homegames-web": "0.9.429",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "electron": "^25.4.0",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1"
  },
  "build": {
    "appId": "homegames",
    "mac": {
      "category": "ayylmao",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "type": "distribution"
    }
  },
  "scripts": {
    "build": "./node_modules/webpack-cli/bin/cli.js --config node_modules/homegames-web/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homegamesio/homegames.git"
  },
  "author": "Joseph Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/homegamesio/homegames/issues"
  },
  "homepage": "https://github.com/homegamesio/homegames#readme"
}
